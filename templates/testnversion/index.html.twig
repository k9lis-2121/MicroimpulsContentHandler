{% extends 'base.html.twig' %}
 
{% block title %}Testnversion index{% endblock %}

{% block body %}
    <h1>Testnversion index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>Message</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for testnversion in testnversions %}
            <tr>
                <td>{{ testnversion.id }}</td>
                <td>{{ testnversion.title }}</td>
                <td>{{ testnversion.message }}</td>
                <td>
                    <a href="{{ path('app_testnversion_show', {'id': testnversion.id}) }}">show</a>
                    <a href="{{ path('app_testnversion_edit', {'id': testnversion.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_testnversion_new') }}">Create new</a>
{% endblock %}
